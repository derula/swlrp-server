    "autoload": {
        "psr-4": {
            "Incertitude\\SWLRP\\": "src/"
        }
    },
    "authors": [
        {
            "name": "derula",
            "email": "derula@uglyhorst.de"
        }
    ],
    "config": {
        "component-dir": "public/components"
    },
    "require": {
        "symfony/yaml": "^3.3",
        "ezyang/htmlpurifier": "^4.9"
    },
    "require-dev": {
        "talyssonoc/php-babel-transpiler": "^0.1.0"
    }
}
